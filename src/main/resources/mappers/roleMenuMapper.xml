<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xuanzy.demo.mapper.RoleMenuMapper">
    <!-- 使用select,insert,update,delete,标签写SQL -->
    <select id="selectByRoleId" resultType="integer">
        select menu_id
        from vd_role_menu where role_id=#{roleId};
    </select>
</mapper>
